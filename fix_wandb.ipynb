   "source": [
    "api = wandb.Api()\n",
    "runs = api.runs(\"natkaha/pollen-retinanet\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "for r in runs:\n",
    "    h = r.history()\n",
    "    r.summary['best_wmAP'] = h['wmAP'].max()\n",
    "    r.summary['best_mAP'] = h['mAP'].max()\n",
    "    r.update()"
   ]
  }
